{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "developmentClient": false,
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release",
        "autoIncrement": false,
        "distribution": "store"
      },
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true,
        "distribution": "store"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
